/Input>\r\n                            </div>\r\n                            <div>\r\n                                <CardText>Toimituspäivämäärä</CardText>\r\n                                <Input placeholder={newTable.toimituspvm}></Input>\r\n                            </div>\r\n\r\n                        </div>\r\n                        <div>\r\n                            <Input onChange={(e) => setAlisatieto(e.target.value)} value={alisatieto} type=\"textarea\" placeholder={newTable.alisatieto}></Input>\r\n                            <Input onChange={(e) => setOstotilaus(e.target.value)} value={ostotilaus} placeholder={\"Ostotilaus\"}></Input>\r\n                        </div>\r\n                    </div>\r\n                    <Table>\r\n                        <Thead>\r\n                            <Tr>\r\n                                <Th>Tuote</Th>\r\n                                <Th>Kerätään</Th>\r\n                                <Th>Keräyspiste</Th>\r\n                                <Th>Lisätietoa</Th>\r\n                            </Tr>\r\n                        </Thead>\r\n                        <Tbody>\r\n                            {newTable.products.map((item) => {\r\n                                return (\r\n                                    <Tr key={item._id}>\r\n                                        <Td><Input id={`kukka/${item._id}`} placeholder={item.kukka}></Input></Td>\r\n                                        <Td><Input id={`toimi/${item._id}`} type=\"number\" placeholder={item.toimi}></Input></Td>\r\n                                        <Td><Input id={`kerays/${item._id}`} type=\"select\" placeholder={item.kerays}>\r\n                                            <option>Ryönä</option>\r\n                                            <option>Tuusjärvi</option>\r\n                                        </Input>\r\n                                        </Td>\r\n                                        <Td><Input id={`lisatieto/${item._id}`} placeholder={item.lisatieto}></Input></Td>\r\n                                    </Tr>\r\n                                )\r\n                            })}\r\n                        </Tbody>\r\n                    </Table>\r\n                    <div className=\"dialogButtons\">\r\n                        <div>\r\n                            <Button onClick={() => addFlowers()} className=\"createFlowersButton\">Lisää kukka</Button>\r\n                            <Input onChange={(e) => setNewFlowers(e.target.value)} value={newFlowers} min={1} max={10} className=\"createFlowersInput\" type=\"number\"></Input>\r\n                        </div>\r\n                        <Button onClick={() => addValuesToTable() + updateProducts() + emptyData()}>Luo taulukko</Button>\r\n                    </div>\r\n                </Card>\r\n            </Dialog> : null}\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Nav;","C:\\Users\\Joonas\\Documents\\Puutarha-app\\Puutarha-Updated-FrontEnd\\src\\components\\Fetch\\Fetch.js",[],"C:\\Users\\Joonas\\Documents\\Puutarha-app\\Puutarha-Updated-FrontEnd\\src\\components\\SearchBar\\searchBar.js",[],"C:\\Users\\Joonas\\Documents\\Puutarha-app\\Puutarha-Updated-FrontEnd\\src\\components\\CardTable\\card.js",[],"C:\\Users\\Joonas\\Documents\\Puutarha-app\\Puutarha-Updated-FrontEnd\\src\\components\\Sockets\\socket-ioConn.js",[],"C:\\Users\\Joonas\\Documents\\Puutarha-app\\Puutarha-Updated-FrontEnd\\src\\components\\NavData\\navData.js",[],"C:\\Users\\Joonas\\Documents\\Puutarha-app\\Puutarha-Updated-FrontEnd\\src\\components\\Table\\table.js",[],"C:\\Users\\Joonas\\Documents\\Puutarha-app\\Puutarha-Updated-FrontEnd\\src\\components\\Sockets\\socketio.js",[],"C:\\Users\\Joonas\\Documents\\Puutarha-app\\Puutarha-Updated-FrontEnd\\src\\components\\Dialog\\dialog.js",["69"],"import { Button } from 'reactstrap';\r\nimport './dialog.css';\r\n\r\nfunction Dialog(props) {\r\n\r\n    let dialog = (\r\n        <div>\r